{
    "manifest_version": 3,
    "name": "Whiteboard Extension",
    "version": "1.0",
    "description": "A browser extension with a whiteboard dashboard.",
    "icons": {
      "16": "images/whiteboard.png",
      "32": "images/whiteboard (1).png",
      "64": "images/whiteboard (2).png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "http://localhost:3000/"
    ]
}
  